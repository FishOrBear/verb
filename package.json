{
  "name": "verb-nurbs-web",
  "version": "2.1.3",
  "author": "pboyer <peter.b.boyer@gmail.com>",
  "description": "A CAD library for the web",
  "declaration": true,
  "types": "typing/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/pboyer/verb.git"
  },
  "scripts": {
    "test": "grunt -v mocha",
    "hx2dts": "hx2dts --haxe /src/verb --destination ./types",
    "join": "file-merge -i **/*.d.ts -e **/index.d.ts -o ./typing/index.d.ts"
  },
  "engine": "node 8.0.0",
  "main": "build/js/verb.js",
  "keywords": [
    "NURBS",
    "geometry",
    "cad",
    "surface",
    "curve",
    "spline",
    "mesh"
  ],
  "dependencies": {},
  "devDependencies": {
    "benchmark": "1.0.0",
    "file-merge": "^1.0.0",
    "grunt": "0.4.5",
    "grunt-benchmark": "0.3.0",
    "grunt-contrib-concat": "0.5.1",
    "grunt-contrib-uglify": "0.9.1",
    "grunt-haxe": "0.1.13",
    "grunt-mocha-test": "0.12.7",
    "grunt-text-replace": "0.4.0",
    "hx2dts": "^0.1.1",
    "mocha": "2.2.4",
    "should": "6.0.1",
    "tess2": "1.0.0"
  },
  "license": "MIT",
  "optionalDependencies": {
    "webworker-threads": "^0.7.12"
  }
}